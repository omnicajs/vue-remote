{
  "name": "@omnicajs/vue-remote",
  "description": "Proxy renderer for Vue.js based on @remote-ui",
  "license": "MIT",
  "version": "0.0.7",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "e2e:build": "node ./tests/e2e/rollup.build.mjs",
    "e2e:serve": "node ./tests/e2e/server.mjs",
    "e2e:test": "npx playwright test",
    "test": "jest --config jest.config.ts ./tests/integration",
    "test:coverage": "jest --config jest.config.ts --coverage --coverageReporters=json",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "@remote-ui/core": "^2.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@jest/types": "^29.6.3",
    "@playwright/test": "^1.41.2",
    "@remote-ui/rpc": "^1.4.4",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^18.19.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vue/compiler-sfc": "^3.4.15",
    "@vue/vue3-jest": "^29.2.6",
    "cors": "^2.8.5",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "playwright": "^1.41.2",
    "rollup": "^4.9.6",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-vue": "^6.0.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "v8-to-istanbul": "^9.2.0",
    "vue": "^3.4.15"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "Vue.js",
    "remote",
    "remote-ui",
    "extension",
    "extending",
    "embed",
    "embedding"
  ],
  "contributors": [
    "Zaitsev Kirill <zaytsev.cmath10@gmail.com>"
  ],
  "homepage": "https://github.com/omnicajs/vue-remote",
  "repository": {
    "type": "git",
    "url": "https://github.com/omnicajs/vue-remote.git"
  }
}
